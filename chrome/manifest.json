{
  "manifest_version": 2,
  "name": "Thumbsup helper for Qzone",
  "description": "This extension mimics clicking thumbsup icon action, relieve you of complaints of friends for not timely thumbsup their posts. ",
  "version": "0.1.1",
  "default_locale": "en",
  "homepage_url": "https://github.com/JerryChin/thumbsup",
  "icons": {
    "16": "images/ThumbFinal_4.9.15.png",
    "48": "images/ThumbFinal_4.9.15.png",
    "128": "images/ThumbFinal_4.9.15.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://user.qzone.qq.com/*",
        "http://user.qzone.qq.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]  
}